{
    "name": "marcelorodrigo/filament-barcode-scanner-field",
    "description": "This is my package filament-barcode-scanner-field",
    "keywords": [
        "marcelorodrigo",
        "laravel",
        "filamentphp",
        "filament-barcode-scanner-field"
    ],
    "homepage": "https://github.com/marcelorodrigo/filament-barcode-scanner-field",
    "support": {
        "issues": "https://github.com/marcelorodrigo/filament-barcode-scanner-field/issues",
        "source": "https://github.com/marcelorodrigo/filament-barcode-scanner-field"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Marcelo Wiebbelling",
            "email": "mrodrigow@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.3",
        "filament/forms": "^4.0",
        "spatie/laravel-package-tools": "^1.15.0"
    },
    "require-dev": {
        "larastan/larastan": "^3.8",
        "laravel/pint": "^1.26",
        "nunomaduro/collision": "^8.8",
        "orchestra/testbench": "^10.0",
        "pestphp/pest": "^4.0",
        "pestphp/pest-plugin-arch": "^4.0",
        "pestphp/pest-plugin-laravel": "^4.0",
        "pestphp/pest-plugin-livewire": "^4.0",
        "phpstan/extension-installer": "^1.4",
        "phpstan/phpstan-deprecation-rules": "^2.0",
        "phpstan/phpstan-phpunit": "^2.0",
        "rector/rector": "^2.3",
        "spatie/laravel-ray": "^1.26"
    },
    "autoload": {
        "psr-4": {
            "Marcelorodrigo\\FilamentBarcodeScannerField\\": "src/",
            "Marcelorodrigo\\FilamentBarcodeScannerField\\Database\\Factories\\": "database/factories/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Marcelorodrigo\\FilamentBarcodeScannerField\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": "@php ./vendor/bin/testbench package:discover --ansi",
        "analyse": "phpstan analyse",
        "lint": "pint",
        "refactor": "rector",
        "test": "pest",
        "test:ci": "vendor/bin/pest --configuration=phpunit.ci.xml --ci",
        "test:coverage": "vendor/bin/pest --configuration=phpunit.ci.xml --ci --coverage",
        "test:lint": "pint --test",
        "test:refactor": "rector --dry-run"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Marcelorodrigo\\FilamentBarcodeScannerField\\FilamentBarcodeScannerFieldServiceProvider"
            ],
            "aliases": {
                "FilamentBarcodeScannerField": "Marcelorodrigo\\FilamentBarcodeScannerField\\Facades\\FilamentBarcodeScannerField"
            }
        }
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "composer/package-versions-deprecated": true,
            "pestphp/pest-plugin": true,
            "phpstan/extension-installer": true
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}